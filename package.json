    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "mocha": "^6.2.1",
    "mongoose": "^5.7.1",
    "jquery": "^3.4.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
